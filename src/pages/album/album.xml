<scroll-view wx:if="{{!isLoading}}" class="list ani-fade-in" scroll-y="{{true}}" enable-back-to-top="{{true}}">
    <navigator class="pic-wrapper" url="/pages/picture/picture?id={{item.id}}\&albumId={{id}}" wx:for="{{pictures}}" wx:key="{{item.id}}">
        <image mode="aspectFill" src="{{item.src}}"/>
    </navigator>
</scroll-view>